{
  "name": "n8n-nodes-bluesky-enhanced",
  "version": "1.7.0",
  "description": "Enhanced BlueSky API nodes for n8n with list management, follower/follows pagination, search functionality, media posting, thread management, reply/quote operations, and analytics features",
  "keywords": [
    "n8n-community-node-package",
    "bluesky",
    "atproto",
    "followers",
    "follows",
    "lists",
    "list-management",
    "content-curation",
    "social-network",
    "search",
    "user-search",
    "post-search",
    "analytics",
    "notifications",
    "engagement",
    "media-posting",
    "video-upload"
  ],
  "license": "MIT",
  "homepage": "https://github.com/brianmoney/n8n-nodes-bluesky#readme",
  "author": {
    "name": "Brian",
    "email": "brian@aspereo.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brianmoney/n8n-nodes-bluesky.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/brianmoney/n8n-nodes-bluesky/issues"
  },
  "engines": {
    "node": ">=20.19",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "clean": "rimraf dist/",
    "build": "n8n-node build",
  "postbuild": "pnpm run build:images",
    "build:legacy": "npm run clean && tsc && npm run build:images",
    "build:images": "copyfiles nodes/**/*.svg nodes/**/*.png dist/",
    "dev": "n8n-node dev --external-n8n",
    "dev:internal": "n8n-node dev",
  "dev:watch": "tsc --watch --preserveWatchOutput",
    "dev:tsc": "tsc --watch",
    "dev:link": "node scripts/dev-link.mjs",
  "start:n8n": "N8N_USER_FOLDER=\"$HOME/.n8n-node-cli/.n8n\" N8N_CUSTOM_EXTENSIONS=\"$HOME/.n8n-node-cli/.n8n/custom\" N8N_PORT=5680 N8N_HOST=0.0.0.0 N8N_LISTEN_ADDRESS=0.0.0.0 n8n start --tunnel",
  "start:dev": "N8N_USER_FOLDER=\"$HOME/.n8n-node-cli/.n8n\" N8N_CUSTOM_EXTENSIONS=\"$HOME/.n8n-node-cli/.n8n/custom\" N8N_PORT=5680 N8N_HOST=0.0.0.0 N8N_LISTEN_ADDRESS=0.0.0.0 N8N_USER_MANAGEMENT_DISABLED=true N8N_DEV_RELOAD=true n8n start --tunnel",
  "start:dev:local": "N8N_USER_FOLDER=\"$HOME/.n8n-node-cli/.n8n\" N8N_CUSTOM_EXTENSIONS=\"$HOME/.n8n-node-cli/.n8n/custom\" N8N_PORT=5680 N8N_HOST=0.0.0.0 N8N_LISTEN_ADDRESS=0.0.0.0 N8N_DEV_RELOAD=true n8n start",
    "format": "prettier nodes credentials --write",
    "lint": "n8n-node lint",
    "lintfix": "n8n-node lint --fix",
  "prepublishOnly": "pnpm build:legacy",
    "release": "n8n-node release"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/BlueskyApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Bluesky/Bluesky.node.js"
    ]
  },
  "devDependencies": {
  "@n8n/node-cli": "^0.5.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@typescript-eslint/parser": "^8.32.1",
    "copyfiles": "^2.4.1",
    "eslint": "^9.35.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.4",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "@atproto/api": "^0.15.8",
    "@atproto/syntax": "^0.4.0",
    "@types/image-size": "^0.8.0",
    "image-size": "^1.2.1",
    "open-graph-scraper": "^6.8.2"
  },
  "pnpm": {
    "overrides": {
      "change-case": "^4.1.2"
    }
  }
}
