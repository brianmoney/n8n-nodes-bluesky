{
  "name": "n8n-nodes-bluesky-enhanced",
  "version": "1.3.0",
  "description": "Enhanced BlueSky API nodes for n8n with follower and follows pagination, search functionality, media posting, thread management, reply/quote operations, and analytics features",
  "keywords": [
    "n8n-community-node-package",
    "bluesky",
    "atproto",
    "followers",
    "follows",
    "social-network",
    "search",
    "user-search",
    "post-search",
    "analytics",
    "notifications",
    "engagement"
  ],
  "license": "MIT",
  "homepage": "https://github.com/brianmoney/n8n-nodes-bluesky#readme",
  "author": {
    "name": "Brian",
    "email": "brian@example.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brianmoney/n8n-nodes-bluesky.git"
  },
  "bugs": {
    "url": "https://github.com/brianmoney/n8n-nodes-bluesky/issues"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "clean": "rimraf dist/",
    "build": "npm run clean && tsc && npm run build:images",
    "build:images": "copyfiles nodes/**/*.svg nodes/**/*.png dist/",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/BlueskyApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Bluesky/Bluesky.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@typescript-eslint/parser": "^8.16.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.4",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "@atproto/api": "^0.15.8",
    "open-graph-scraper": "^6.8.2"
  }
}
